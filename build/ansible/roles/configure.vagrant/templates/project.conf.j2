# {{ ansible_managed }}
server {
    listen 80;
    server_name _;

    access_log off;
    error_log off;

    client_max_body_size 0;

    proxy_buffering off;
    proxy_set_header Host $host;
    proxy_set_header X-Forwarded-For $remote_addr;

    location / {
        proxy_pass http://127.0.0.1:7480;
    }
}
